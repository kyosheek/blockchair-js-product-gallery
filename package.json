{
  "name": "@kyoshee/blockchair-js-product-gallery",
  "version": "1.1.0",
  "private": false,
  "description": "Injectable gallery of Blockchair products",
  "keywords": [
    "javascript",
    "blockchair"
  ],
  "repository": {
    "type": "git",
    "url": "."
  },
  "license": "GPL-3.0-only",
  "author": "kyoshee",
  "type": "module",
  "main": "./src/index.js",
  "directories": {
    "test": "test"
  },
  "types": "src/types.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "src/types.d.ts"
      ],
      "*.svg?url": [
        "src/svg.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./src/index.js",
    "./*.svg": "./src/assets/*.svg"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "vite": "7.1.11"
  }
}
